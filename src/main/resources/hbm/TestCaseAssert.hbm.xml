<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.domain.sys">
						
	<class name="TestCaseAssert" table="[test case assert]">
		
		<id name="id" column="[id]">
			<generator class="native" />
		</id>					
				
		
		<property name="testId" column="[test_id]"/>
		<property name="sourceClass" column="[source_class]" />
		<property name="assertHashCode" column="[assertive_hash_code]" />
		<property name="hasErrors" column="[has_errors]" />
										
	</class>
</hibernate-mapping>
