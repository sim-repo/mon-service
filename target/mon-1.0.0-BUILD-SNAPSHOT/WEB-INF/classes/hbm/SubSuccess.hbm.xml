<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.domain.sys">
						
	<class name="LogSubSuccess" table="[log sub success]">
		
		<id name="id" column="[id]">
			<generator class="native" />
		</id>
		
		<property name="juuid" column="[uuid]"/>
		<property name="eventId" column="[event_id]"/>
		<property name="senderId" column="[sender_id]"/>
		<property name="publisherId" column="[publisher_id]"/>					
	</class>
</hibernate-mapping>				