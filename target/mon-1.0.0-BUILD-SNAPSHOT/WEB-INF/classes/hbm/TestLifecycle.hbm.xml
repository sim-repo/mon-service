<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.job">
						
	<class name="LifecycleJob" table="[test lifecycle]">
		
		<id name="testId" column="[test_id]">
			<generator class="assigned" />
		</id>					
		
		
		<property name="juuid" column="uuid" />
		<property name="logDatetime" column="[log_datetime]"/>
		<property name="eventId" column="[event_id]"/>		
		<property name="messageBodyValue" column="[msg]"/>
						
	</class>
</hibernate-mapping>
