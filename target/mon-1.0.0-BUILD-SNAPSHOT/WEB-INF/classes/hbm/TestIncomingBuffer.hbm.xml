<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.domain.contract">
						
	<class name="UniMinMsg" table="[test incoming buffer]">				
		<id name="id" column="[id]">
			<generator class="native" />
		</id>
		
		
		<property name="juuid" column="uuid" />
		<property name="datetime" column="[log_datetime]"/>
		<property name="eventId" column="[event_id]"/>		
		<property name="originalRequestBody" column="[msg]"/>				
						
	</class>
</hibernate-mapping>
